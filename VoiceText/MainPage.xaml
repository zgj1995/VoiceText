<Page
    x:Class="VoiceText.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:VoiceText"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <ComboBox Width="400" x:Name="Select">
                </ComboBox>
                <ComboBox Width="400" x:Name="Select2">
                </ComboBox>
            </StackPanel>
            <TextBox x:Name="Content"/>
            <Button Content="测试" x:Name="Text" Click="Text_Click"/>
            <MediaElement x:Name="Me">
            </MediaElement>
        </StackPanel>
    </Grid>
</Page>
